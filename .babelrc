{
  "sourceMaps": "inline",
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "corejs": 2,
      }
    ], 
    ["@babel/typescript"]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties", 
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ]
  ]
}